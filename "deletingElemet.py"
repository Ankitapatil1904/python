l1 = [10, 20, -30, "40", "Fifty", 60.0, 7.0, [80, "80", 80.0, "Eighty", 80j], 90+0j, "100"]
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
del l1[2]
print("\nAfter deleting an element from index number 2 :")
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
del l1[4]
print("\nAfter deleting an element from index number 4 :")
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
del l1[1]
print("\nAfter deleting an element from index number 1 :")
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
del l1[-3]
print("\nAfter deleting an element from index number -3 :")
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
del l1[-1]
print("\nAfter deleting an element from index number -1 :")
print("Identifier l1 :", l1, "Type :", type(l1), "ID :", id(l1), "Length :", len(l1))
print("\nThank you.")
